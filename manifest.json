{
  "manifest_version": 2,
  "name": "Tidepool Uploader",
  "short_name": "Uploader",
  "version": "0.99.0",
  "description": "The Tidepool Uploader helps you get your data from insulin pumps, CGMs and BG meters into Tidepoolâ€™s secure cloud platform.",
  "minimum_chrome_version": "38",
  "icons": {
    "16": "T_icon_dark_16.png",
    "32": "T_icon_dark_32.png",
    "64": "T_icon_dark_64.png",
    "128": "T_icon_dark_128.png",
    "512": "T_icon_dark_512.png"
  },
  "permissions": [
    "http://localhost/",
    "https://*.tidepool.io/",
    "fileSystem",
    "system.storage",
    "storage",
    "serial",
    "usb",
    {
      "usbDevices": [
        {
          "deviceName": "Dexcom G4 CGM",
          "driverId": "DexcomG4",
          "mode": "serial",
          "serialPortPat": "/dev/cu\\.usbmodem[A-Za-z0-9]+",
          "vendorId": 8867,
          "productId": 71
        },
        {
          "deviceName": "Asante SNAP w/Asante cable",
          "driverId": "AsanteSNAPNeedDriver",
          "mode": "not supported",
          "vendorId": 1027,
          "productId": 32568
        },
        {
          "deviceName": "Asante SNAP w/FTDI cable",
          "driverId": "AsanteSNAP",
          "serialPortPat": "/dev/cu\\.usbserial-[A-Za-z0-9]+",
          "mode": "FTDI",
          "vendorId": 1027,
          "productId": 24577
        },
        {
          "deviceName": "OneTouch Mini w/FTDI cable",
          "driverId": "OneTouchMini",
          "serialPortPat": "/dev/cu\\.usbserial-[A-Za-z0-9]+",
          "mode": "FTDI",
          "vendorId": 1027,
          "productId": 24577
        },
        {
          "deviceName": "Insulet OmniPod",
          "driverId": "InsuletOmniPod",
          "mode": "block",
          "vendorId": 7734,
          "productId": 2
        }
      ]
    }
  ],
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  }
}
